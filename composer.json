{
    "name": "socialveo/socialveo-docs-gen",
    "description": "API Documentation generator for the Socialveo",
    "keywords": ["socialveo", "phpdoc", "apidoc", "api", "documentation"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/socialveo/socialveo-docs-gen/issues",
        "source": "https://github.com/socialveo/socialveo-docs-gen"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        },
        {
            "name": "Andrey Izman",
            "email": "izmanw@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "yiisoft/yii2": "~2.0.4",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "phpdocumentor/reflection": "^3.0.1",
        "phpdocumentor/reflection-docblock": "^2.0.4",
        "nikic/php-parser": "^1.0",
        "cebe/js-search": "~0.9.3",
        "cebe/markdown": "~1.0.0 | ~1.1.0",
        "cebe/markdown-latex": "~1.0",
        "scrivo/highlight.php": "~8.0",
        "erusev/parsedown": "dev-master",
        "michelf/php-markdown": "^1.7",
        "socialveo/socialveo": "dev-master",
        "socialveo/socialveo-docs": "dev-master"
    },
    "autoload": {
        "psr-4": { "yii\\apidoc\\": "" }
    },
    "bin": ["apidoc"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:socialveo/socialveo.git",
            "reference": "origin/master"
        },
        {
            "type": "vcs",
            "url": "git@github.com:socialveo/socialveo-docs.git",
            "reference": "origin/master"
        }
    ]
}
